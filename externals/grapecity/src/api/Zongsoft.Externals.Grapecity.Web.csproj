    <OutputType>Library</OutputType>
    <Authors>Popeye Zhong</Authors>
    <Company>Zongsoft Studio</Company>
    <Description>This is a library about GrapeCity Web SDK.</Description>
    <Copyright>Copyright(C) Zongsoft Studio 2020-2021. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/Zongsoft</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Zongsoft/Framework</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GrapeCity.ActiveReports" Version="15.1.5" />
    <PackageReference Include="GrapeCity.ActiveReports.Aspnetcore.Viewer" Version="15.1.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Zongsoft.Externals.Grapecity.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Zongsoft.Core">
      <HintPath>..\..\..\..\Zongsoft.Core\src\bin\$(Configuration)\net5.0\Zongsoft.Core.dll</HintPath>
    </Reference>
    <Reference Include="Zongsoft.Reporting">
      <HintPath>..\..\..\..\Zongsoft.Reporting\src\bin\$(Configuration)\net5.0\Zongsoft.Reporting.dll</HintPath>
    </Reference>
  </ItemGroup>
