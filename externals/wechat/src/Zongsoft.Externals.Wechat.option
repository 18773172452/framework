<?xml version="1.0" encoding="utf-8" ?>

<options>
	<option path="/Externals/Wechat">
		<platform>
			<apps default="AppId">
				<app app.name="AppId" secret="AppSecret" nonce="Token" key="SymmetricKey" />
			</apps>
		</platform>

		<applet>
			<apps default="AppId">
				<app app.name="AppId" secret="AppSecret" />
			</apps>
		</applet>

		<channel>
			<apps>
				<app app.name="AppId" secret="AppSecret" />
			</apps>
		</channel>
	</option>
</options>
