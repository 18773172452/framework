<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<Authors>Popeye Zhong</Authors>
		<Company>Zongsoft Studio</Company>
		<Description>This is a library about Wechat SDK.</Description>
		<Copyright>Copyright(C) Zongsoft Studio 2015-2020. All rights reserved.</Copyright>
		<PackageProjectUrl>https://github.com/Zongsoft</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Zongsoft/Framework</RepositoryUrl>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="api\**" />
	  <Compile Remove="guarding\**" />
	  <EmbeddedResource Remove="api\**" />
	  <EmbeddedResource Remove="guarding\**" />
	  <None Remove="api\**" />
	  <None Remove="guarding\**" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="5.0.0" />
		<PackageReference Include="Microsoft.Extensions.Http" Version="5.0.0" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="Zongsoft.Core">
			<HintPath>..\..\..\Zongsoft.Core\src\bin\$(Configuration)\net5.0\Zongsoft.Core.dll</HintPath>
		</Reference>
	</ItemGroup>
	<ItemGroup>
	  <Compile Update="Properties\Resources.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>
	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <Generator>ResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>
</Project>
