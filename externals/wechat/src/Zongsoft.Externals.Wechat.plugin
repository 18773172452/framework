<?xml version="1.0" encoding="utf-8" ?>

<plugin name="Zongsoft.Externals.Wechat"
        title="Zongsoft.Externals.Wechat Plugin"
        author="Zongsoft Studio(zongsoft@qq.com)"
        copyright="Copyright(C) Zongsoft Studio. 2015-2025. All rights reserved."
        description="Created Date: 2019-08-10 by Popeye Zhong" >

	<manifest>
		<assemblies>
			<assembly name="Zongsoft.Externals.Wechat" />
		</assemblies>
	</manifest>

	<extension path="/Workbench/Wechat">
		<object name="Applets" value="{type:Zongsoft.Externals.Wechat.AppletManager, Zongsoft.Externals.Wechat}" />
		<object name="Channels" value="{type:Zongsoft.Externals.Wechat.ChannelManager, Zongsoft.Externals.Wechat}" />
	</extension>
</plugin>
