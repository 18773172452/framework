<?xml version="1.0" encoding="utf-8" ?>

<plugin name="Zongsoft.Externals.Scriban"
        title="Zongsoft.Externals.Scriban Plugin"
        author="Zongsoft Studio"
        version="1.0"
        description="Create by: Popeye Zhong at 2024-08-10" >

	<manifest>
		<assemblies>
			<assembly name="Zongsoft.Externals.Scriban" />
		</assemblies>
	</manifest>

	<extension path="/Workbench/Expressions">
		<object name="Scriban" type="Zongsoft.Externals.Scriban.ScribanExpressionParser, Zongsoft.Externals.Scriban">
			<object.property name="Default" target="{type:Zongsoft.Expressions.Expression, Zongsoft.Core}" value="{path:.}" />
		</object>
	</extension>
</plugin>